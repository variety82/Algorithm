def solution(lottos, win_nums):
    arr = [x for x in lottos if x in win_nums]
    cnt = lottos.count(0)
    print(cnt + len(arr))
    
    rank = [6, 6, 5, 4, 3, 2, 1]
    answer = [rank[cnt+len(arr)], rank[len(arr)]]
    # print(arr)
    return answer
    
    
    
    
